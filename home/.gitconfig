[user]
	name = Simon Eskildsen
	email = sirup@sirupsen.com
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = auto
[merge]
  tool = vimdiff
[core]
  editor = /usr/bin/vim
  excludesfile = /opt/boxen/config/git/gitignore
[interactive]
  singlekey = true
[push]
  default = tracking
[credential]
	helper = /opt/boxen/bin/boxen-git-credential
[pull]
  rebase = true

[hub]
protocol = https
